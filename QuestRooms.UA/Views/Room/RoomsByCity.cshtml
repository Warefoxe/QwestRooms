
@model IEnumerable<QuestRooms.BLL.DTOModels.RoomDto>

<!DOCTYPE html>

<div class="row">
    @foreach (var rooms in Model)
    {
        <div class="col-xl-3 col-lg-4 col-md-6 col-12">
            <div class="card">
                <div class="card-img-top">
                    <img class="img-fluid" src="https://q-18.com/wp-content/uploads/2018/08/posip-1024x720.jpg" />
                </div>
                <div class="card-header">
                    @rooms.Name
                </div>
                <div class="card-text">
                    <div>
                        @rooms.Description
                    </div>
                    <div>
                        @rooms.Email
                    </div>
                </div>
                <div class="card-footer">
                    Buy
                </div>
            </div>
        </div>
    }
</div>
